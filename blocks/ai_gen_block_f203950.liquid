{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-video-gallery-{{ ai_gen_id }} {
    width: 100%;
    padding: 40px 20px;
    background-color: {{ block.settings.background_color }};
  }

  .ai-video-gallery-container-{{ ai_gen_id }} {
    max-width: 1400px;
    margin: 0 auto;
  }

  .ai-video-gallery-header-{{ ai_gen_id }} {
    margin-bottom: 30px;
  }

  .ai-video-gallery-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.text_color }};
    margin: 0 0 20px;
  }

  .ai-video-gallery-tabs-{{ ai_gen_id }} {
    display: flex;
    gap: 10px;
    flex-wrap: wrap;
    border-bottom: 2px solid {{ block.settings.border_color }};
    padding-bottom: 10px;
    margin-bottom: 30px;
  }

  .ai-video-gallery-tab-{{ ai_gen_id }} {
    padding: 12px 24px;
    background-color: {{ block.settings.tab_background }};
    color: {{ block.settings.tab_text_color }};
    border: 1px solid {{ block.settings.border_color }};
    border-radius: {{ block.settings.tab_border_radius }}px;
    cursor: pointer;
    font-size: 16px;
    transition: all 0.3s ease;
  }

  .ai-video-gallery-tab-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.tab_hover_background }};
  }

  .ai-video-gallery-tab-{{ ai_gen_id }}.active {
    background-color: {{ block.settings.tab_active_background }};
    color: {{ block.settings.tab_active_text_color }};
    border-color: {{ block.settings.tab_active_background }};
  }

  .ai-video-gallery-content-{{ ai_gen_id }} {
    display: none;
  }

  .ai-video-gallery-content-{{ ai_gen_id }}.active {
    display: block;
  }

  .ai-video-gallery-layout-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr 350px;
    gap: 20px;
  }

  .ai-video-gallery-main-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 20px;
  }

  .ai-video-gallery-player-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    padding-bottom: 56.25%;
    background-color: #000000;
    border-radius: {{ block.settings.video_border_radius }}px;
    overflow: hidden;
  }

  .ai-video-gallery-player-{{ ai_gen_id }} iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border: none;
  }

  .ai-video-gallery-placeholder-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #ffffff;
    font-size: 16px;
    text-align: center;
    padding: 20px;
  }

  .ai-video-gallery-description-{{ ai_gen_id }} {
    padding: 20px;
    background-color: {{ block.settings.description_background }};
    border-radius: {{ block.settings.description_border_radius }}px;
    border: 1px solid {{ block.settings.border_color }};
  }

  .ai-video-gallery-collection-title-{{ ai_gen_id }} {
    font-size: 24px;
    color: {{ block.settings.text_color }};
    margin: 0 0 10px;
  }

  .ai-video-gallery-collection-desc-{{ ai_gen_id }} {
    font-size: 14px;
    color: {{ block.settings.text_color }};
    line-height: 1.6;
  }

  .ai-video-gallery-sidebar-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 12px;
    max-height: 600px;
    overflow-y: auto;
    padding-right: 10px;
  }

  .ai-video-gallery-sidebar-{{ ai_gen_id }}::-webkit-scrollbar {
    width: 8px;
  }

  .ai-video-gallery-sidebar-{{ ai_gen_id }}::-webkit-scrollbar-track {
    background: {{ block.settings.scrollbar_track }};
    border-radius: 4px;
  }

  .ai-video-gallery-sidebar-{{ ai_gen_id }}::-webkit-scrollbar-thumb {
    background: {{ block.settings.scrollbar_thumb }};
    border-radius: 4px;
  }

  .ai-video-gallery-sidebar-{{ ai_gen_id }}::-webkit-scrollbar-thumb:hover {
    background: {{ block.settings.scrollbar_thumb_hover }};
  }

  .ai-video-gallery-thumbnail-{{ ai_gen_id }} {
    display: flex;
    gap: 12px;
    padding: 10px;
    background-color: {{ block.settings.thumbnail_background }};
    border: 2px solid transparent;
    border-radius: {{ block.settings.thumbnail_border_radius }}px;
    cursor: pointer;
    transition: all 0.3s ease;
  }

  .ai-video-gallery-thumbnail-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.thumbnail_hover_background }};
    border-color: {{ block.settings.border_color }};
  }

  .ai-video-gallery-thumbnail-{{ ai_gen_id }}.active {
    border-color: {{ block.settings.tab_active_background }};
    background-color: {{ block.settings.thumbnail_active_background }};
  }

  .ai-video-gallery-thumb-image-{{ ai_gen_id }} {
    position: relative;
    width: 120px;
    height: 68px;
    flex-shrink: 0;
    background-color: #000000;
    border-radius: 4px;
    overflow: hidden;
  }

  .ai-video-gallery-thumb-image-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-video-gallery-thumb-info-{{ ai_gen_id }} {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
  }

  .ai-video-gallery-thumb-title-{{ ai_gen_id }} {
