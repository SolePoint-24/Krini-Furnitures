{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-partner-carousel-{{ ai_gen_id }} {
    width: 100%;
    overflow: hidden;
    padding: {{ block.settings.section_padding_top }}px 0 {{ block.settings.section_padding_bottom }}px;
    background-color: {{ block.settings.background_color }};
  }

  .ai-partner-carousel__container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-partner-carousel__header-{{ ai_gen_id }} {
    text-align: {{ block.settings.heading_alignment }};
    margin-bottom: 40px;
  }

  .ai-partner-carousel__heading-{{ ai_gen_id }} {
    font-size: {{ block.settings.heading_size }}px;
    color: {{ block.settings.heading_color }};
    margin: 0;
  }

  .ai-partner-carousel__wrapper-{{ ai_gen_id }} {
    position: relative;
    padding: 10px 50px;
  }

  .ai-partner-carousel__track-{{ ai_gen_id }} {
    display: flex;
    gap: {{ block.settings.card_gap }}px;
    overflow-x: auto;
    scroll-behavior: smooth;
    scrollbar-width: none;
    -ms-overflow-style: none;
    padding: 5px 0;
  }

  .ai-partner-carousel__track-{{ ai_gen_id }}::-webkit-scrollbar {
    display: none;
  }

  .ai-partner-carousel__card-{{ ai_gen_id }} {
    flex: 0 0 calc((100% - {{ block.settings.card_gap | times: 3 }}px) / 4);
    min-width: 200px;
    background-color: {{ block.settings.card_background }};
    border-radius: {{ block.settings.card_border_radius }}px;
    padding: {{ block.settings.card_padding }}px;
    text-align: center;
    border: {{ block.settings.card_border_width }}px solid {{ block.settings.card_border_color }};
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-partner-carousel__card-{{ ai_gen_id }}:hover {
    transform: translateY(-5px);
    box-shadow: 0 8px 20px rgba(0, 0, 0, 0.1);
  }

  .ai-partner-carousel__card-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.card_title_size }}px;
    color: {{ block.settings.card_title_color }};
    margin: 0;
    font-weight: 600;
    text-align: center;
  }

  .ai-partner-carousel__nav-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    width: 40px;
    height: 40px;
    border-radius: 50%;
    background-color: {{ block.settings.nav_button_background }};
    border: none;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: background-color 0.3s ease;
    z-index: 2;
  }

  .ai-partner-carousel__nav-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.nav_button_hover_background }};
  }

  .ai-partner-carousel__nav-{{ ai_gen_id }}:disabled {
    opacity: 0.3;
    cursor: not-allowed;
  }

  .ai-partner-carousel__nav--prev-{{ ai_gen_id }} {
    left: 0;
  }

  .ai-partner-carousel__nav--next-{{ ai_gen_id }} {
    right: 0;
  }

  .ai-partner-carousel__nav-{{ ai_gen_id }} svg {
    width: 20px;
    height: 20px;
    fill: {{ block.settings.nav_button_icon_color }};
  }

  @media screen and (max-width: 989px) {
    .ai-partner-carousel__card-{{ ai_gen_id }} {
      flex: 0 0 calc((100% - {{ block.settings.card_gap | times: 2 }}px) / 3);
    }

    .ai-partner-carousel__heading-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.heading_size }}px * 0.8);
    }
  }

  @media screen and (max-width: 749px) {
    .ai-partner-carousel__wrapper-{{ ai_gen_id }} {
      padding: 10px 40px;
    }

    .ai-partner-carousel__card-{{ ai_gen_id }} {
      flex: 0 0 calc((100% - {{ block.settings.card_gap }}px) / 2);
      min-width: 150px;
    }

    .ai-partner-carousel__nav-{{ ai_gen_id }} {
      width: 32px;
      height: 32px;
    }

    .ai-partner-carousel__nav-{{ ai_gen_id }} svg {
      width: 16px;
      height: 16px;
    }

    .ai-partner-carousel__heading-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.heading_size }}px * 0.6);
    }
  }
{% endstyle %}

<partner-carousel-{{ ai_gen_id }}
  class="ai-partner-carousel-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-partner-carousel__container-{{ ai_gen_id }}">
    {% if block.settings.heading != blank %}
      <div class="ai-partner-carousel__header-{{ ai_gen_id }}">
        <h2 class="ai-partner-carousel__heading-{{ ai_gen_id }}">{{ block.settings.heading }}</h2>
      </div>
    {% endif %}

    <div class="ai-partner-carousel__wrapper-{{ ai_gen_id }}">
      <button
        class="ai-partner-carousel__nav-{{ ai_gen_id }} ai-partner-carousel__nav--prev-{{ ai_gen_id }}"
        aria-label="Previous"
        data-direction="prev"
      >
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
          <path d="M15.41 7.41L14 6l-6 6 6 6 1.41-1.41L10.83 12z"/>
        </svg>
      </button>

      <div class="ai-partner-carousel__track-{{ ai_gen_id }}">
        {% if block.settings.card_1_title != blank %}
          <div class="ai-partner-carousel__card-{{ ai_gen_id }}">
            <h3 class="ai-partner-carousel__card-title-{{ ai_gen_id }}">{{ block.settings.card_1_title }}</h3>
          </div>
        {% endif %}

        {% if block.settings.card_2_title != blank %}
          <div class="ai-partner-carousel__card-{{ ai_gen_id }}">
            <h3 class="ai-partner-carousel__card-title-{{ ai_gen_id }}">{{ block.settings.card_2_title }}</h3>
          </div>
        {% endif %}

        {% if block.settings.card_3_title != blank %}
          <div class="ai-partner-carousel__card-{{ ai_gen_id }}">
            <h3 class="ai-partner-carousel__card-title-{{ ai_gen_id }}">{{ block.settings.card_3_title }}</h3>
          </div>
        {% endif %}

        {% if block.settings.card_4_title != blank %}
          <div class="ai-partner-carousel__card-{{ ai_gen_id }}">
            <h3 class="ai-partner-carousel__card-title-{{ ai_gen_id }}">{{ block.settings.card_4_title }}</h3>
          </div>
        {% endif %}

        {% if block.settings.card_5_title != blank %}
          <div class="ai-partner-carousel__card-{{ ai_gen_id }}">
            <h3 class="ai-partner-carousel__card-title-{{ ai_gen_id }}">{{ block.settings.card_5_title }}</h3>
          </div>
        {% endif %}

        {% if block.settings.card_6_title != blank %}
          <div class="ai-partner-carousel__card-{{ ai_gen_id }}">
            <h3 class="ai-partner-carousel__card-title-{{ ai_gen_id }}">{{ block.settings.card_6_title }}</h3>
          </div>
        {% endif %}

        {% if block.settings.card_7_title != blank %}
          <div class="ai-partner-carousel__card-{{ ai_gen_id }}">
            <h3 class="ai-partner-carousel__card-title-{{ ai_gen_id }}">{{ block.settings.card_7_title }}</h3>
          </div>
        {% endif %}
      </div>

      <button
        class="ai-partner-carousel__nav-{{ ai_gen_id }} ai-partner-carousel__nav--next-{{ ai_gen_id }}"
        aria-label="Next"
        data-direction="next"
      >
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
          <path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6z"/>
        </svg>
      </button>
    </div>
  </div>
</partner-carousel-{{ ai_gen_id }}>

<script>
  (function() {
    class PartnerCarousel{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.track = this.querySelector('.ai-partner-carousel__track-{{ ai_gen_id }}');
        this.cards = this.querySelectorAll('.ai-partner-carousel__card-{{ ai_gen_id }}');
        this.prevButton = this.querySelector('.ai-partner-carousel__nav--prev-{{ ai_gen_id }}');
        this.nextButton = this.querySelector('.ai-partner-carousel__nav--next-{{ ai_gen_id }}');
      }

      connectedCallback() {
        if (this.cards.length === 0) return;

        this.setupNavigation();
        this.updateButtons();
      }

      setupNavigation() {
        this.prevButton.addEventListener('click', () => this.scroll('prev'));
        this.nextButton.addEventListener('click', () => this.scroll('next'));

        this.track.addEventListener('scroll', () => {
          this.updateButtons();
        });
      }

      scroll(direction) {
        const cardWidth = this.cards[0].offsetWidth;
        const gap = {{ block.settings.card_gap }};
        const scrollAmount = cardWidth + gap;

        if (direction === 'next') {
          this.track.scrollLeft += scrollAmount;
        } else {
          this.track.scrollLeft -= scrollAmount;
        }
      }

      updateButtons() {
        const maxScroll = this.track.scrollWidth - this.track.clientWidth;
        this.prevButton.disabled = this.track.scrollLeft <= 0;
        this.nextButton.disabled = this.track.scrollLeft >= maxScroll - 1;
      }
    }

    customElements.define('partner-carousel-{{ ai_gen_id }}', PartnerCarousel{{ ai_gen_id }});
  })();
</script>

