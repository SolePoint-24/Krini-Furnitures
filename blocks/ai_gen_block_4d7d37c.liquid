        <div class="ai-catalog-modal-description-{{ ai_gen_id }}">
          {{ block.settings.modal_description }}
        </div>
      {% endif %}

      <div class="ai-catalog-info-box-{{ ai_gen_id }}">
        <p>
          <strong>ðŸ“Š How your data is stored:</strong>
          Form submissions are automatically saved in your Shopify admin under Settings â†’ Customer events. All catalog download requests are tagged with "catalog_download" for easy filtering and follow-up.
        </p>
      </div>

      {% form 'contact' %}
        <div class="ai-catalog-form-{{ ai_gen_id }}">
          <input type="hidden" name="contact[catalog_download]" value="true">

          {% if form.posted_successfully? %}
